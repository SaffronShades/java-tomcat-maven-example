pipeline {

	agent any
	
	stages {
	
		stage ('Initialize') {
			steps {				
				echo 'Initializing Code File'				
			}				
		}
		
		stage ('Build') {
			steps {
				echo 'Initializing Build File'
			}
		}
		
		stage ('Deploy the artifact') {
			steps {
				echo 'Deploying the artifact'
			}
		}
	}

}